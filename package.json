{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:server": "node dist/server/main.cjs",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:server\"",
    "build:frontend": "tsc && vite build",
    "build:server": "rm -rf dist/server && tsc -p server/tsconfig.build.json && find dist/server -name \"*.js\" -type f -exec bash -c 'mv \"$1\" \"${1%.js}.cjs\"' _ {} \\; && node scripts/fix-module-imports.mjs",
    "build": "npm run build:frontend && npm run build:server",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "strapi:dev": "cd my-strapi-backend && npm run develop",
    "start:server": "node dist/server/main.cjs",
    "start": "node dist/server/main.cjs",
    "start:strapi": "cd my-strapi-backend && npm run develop",
    "start:backend": "npm run start:server",
    "start:frontend": "npm run dev:frontend",
    "start:backend:wait": "wait-on tcp:1337 -t 120000 && npm run start:backend",
    "start:frontend:wait": "wait-on tcp:3001 -t 30000 && npm run start:frontend",
    "start:all": "concurrently --kill-others \"npm:start:strapi\" \"npm:start:backend:wait\" \"npm:start:frontend:wait\"",
    "start:express": "cd server-express && node index.mjs",
    "start:express:dev": "cd server-express && nodemon index.mjs",
    "start:express:wait": "wait-on tcp:1337 -t 120000 && npm run start:express",
    "start:all:express": "concurrently --kill-others \"npm:start:strapi\" \"npm:start:express:wait\" \"npm:start:frontend:wait\"",
    "stop:all": "./stop-all-services.sh",
    "build:prod": "NODE_ENV=production npm run build",
    "start:strapi:prod": "cd my-strapi-backend && npm run start",
    "start:express:prod": "cd server-express && NODE_ENV=production node index.mjs",
    "start:frontend:prod": "vite preview --port 8083",
    "deploy": "./deploy-baota.sh"
  },
