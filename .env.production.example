# 前端应用配置 | Frontend Application Configuration
VITE_API_URL=http://your-domain.com/api
VITE_STRAPI_URL=http://your-domain.com:1337
VITE_PUBLIC_URL=http://your-domain.com

# 关闭模拟数据和文件系统，仅使用 Strapi API | Turn off mock data and file system, use only Strapi API
VITE_USE_MOCK_DATA=false
VITE_USE_FILE_SYSTEM=false
VITE_STATIC_MODE=false

# Strapi API Token | Strapi API Token
STRAPI_API_TOKEN=your-strapi-api-token

# Express中间层配置 | Express Middleware Configuration
EXPRESS_PORT=3001
STRAPI_URL=http://localhost:1337
STRAPI_API_URL=http://localhost:1337
CORS_ORIGIN=http://your-domain.com

# 其他生产环境特定配置 | Other Production Environment Specific Configuration
NODE_ENV=production
PROXY_PORT=3001
